#include<iostream>
using namespace std;

class sum {
    public:
    
    int getSum(int n){
        int sum=0;
        while(n!=0){
            sum= sum+ n%10;
            n=n/10;
        }
        return sum;
    }
};


int main(){
    sum s;
    int no;
    cout<<"enter no";
    cin>>no;
    
    cout<<"Sum is "<<s.getSum(no);
}
